#include <stdio.h>
 int main(){
     char a;
     printf("enter the text\n");
     scanf("%c",&a);
     if ( a>=65 && a<=90)
     printf("upper case alphabet");
     else if ( a>=97 && a<=122)
     printf("lower case alphabet");
     else if ( a>=48 && a<=57)
     printf(" its a number");
     else 
     printf("its a special character");
     return 0;
 }
